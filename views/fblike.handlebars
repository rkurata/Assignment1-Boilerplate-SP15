<h1 id="title">Facebook Likes</h1><br>

<body>
   
 {{#each fblike}}
  	<div class="feed" id="info">

    <script type="text/javascript"> 
        var d = {{year}};
        var count = 0;
        for (d == 2013) 
            { 
              count++;
              document.write(count);
            } 
    </script> 

  <p>
    Category: {{category}}<br>
   Item: {{name}}<br>
    Liked on: {{created_time}}<br>
    </p>
  
  </div>
  {{/each}}

  <center><button class="nav"  onclick="window.location.href='/facebook'">Photos</button></center>

</body>
